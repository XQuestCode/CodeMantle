# CodeMantle Agent Daemon - npm configuration
# This package is published to npm registry

# Use npm registry
registry=https://registry.npmjs.org

# Engine strict mode to ensure Node.js version compatibility
engine-strict=true

# Save exact versions instead of using semver ranges
save-exact=true

# Disable funding messages during install
fund=false

# Disable audit during install (run separately)
audit=false

# Lockfile version
lockfile-version=3
