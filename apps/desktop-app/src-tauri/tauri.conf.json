{
  "identifier": "com.codemantle.desktop",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "webviewInstallMode": {
        "type": "embedBootstrapper",
        "silent": true
      }
    }
  },
  "productName": "CodeMantle",
  "version": "0.1.8",
  "plugins": {
    "autostart": {
      "enabled": true
    },
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDMzNDFBQzgyRDNCOEU0NjMKUldSajVMalRncXhCTXpYc1ZDaGkwbEU1Rkk2bnkxMjBwZU5QblRKcHlsSlpmaEhXbDdPS3ZvWkMK",
      "endpoints": [
        "https://github.com/XQuestCode/CodeMantle/releases/latest/download/latest.json"
      ]
    }
  },
  "app": {
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "default",
          "windows": [
            "main"
          ],
          "permissions": [
            "core:default",
            "dialog:default",
            "shell:default",
            "autostart:default",
            "updater:default",
            "shell:allow-spawn",
            "shell:allow-execute",
            "shell:allow-kill"
          ]
        }
      ]
    },
    "windows": [
      {
        "label": "main",
        "title": "CodeMantle Setup",
        "width": 800,
        "height": 600,
        "resizable": false,
        "maximizable": false,
        "minimizable": true,
        "center": true,
        "visible": true
      }
    ]
  }
}