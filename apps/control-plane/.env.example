PANEL_ENV_SCHEMA_VERSION=1
VALID_TOKENS=replace_with_secure_token
JIT_CREDENTIAL_SIGNING_KEY=replace_with_32_byte_base64url_secret
CONTROL_PLANE_PORT=8787
CONTROL_PLANE_API_PORT=8788
HEARTBEAT_SECONDS=25
REQUEST_TIMEOUT_MS=15000
MCP_GATEWAY_BASE_URLS=shared=https://mcp-gateway.internal
MAX_API_BODY_BYTES=4096
MAX_PROMPT_CHARS=8000
AUTH_MODE=local
AUTH_MFA_ENABLED=true
# TOTP implementation is Authy-compatible RFC6238
AUTH_MFA_PROVIDER=authy
# If true, all local users must have a TOTP passkey configured
AUTH_MFA_REQUIRE_FOR_ALL_USERS=false
AUTH_OWNER_EMAIL=owner@example.com
# Recommended: set pre-hashed password (scrypt format)
AUTH_OWNER_PASSWORD_HASH=scrypt$16384$8$1$replace_salt$replace_hash
# Optional plain password (hash generated at runtime)
# AUTH_OWNER_PASSWORD=change_me_now
AUTH_OWNER_ROLE=owner
# Recommended explicit 2FA passkey (Base32) for owner account
# AUTH_OWNER_2FA_PASSKEY=JBSWY3DPEHPK3PXP
# Optional legacy alias still supported
# Optional TOTP secret in Base32 for MFA (Authy-compatible RFC6238)
# AUTH_OWNER_TOTP_SECRET=JBSWY3DPEHPK3PXP
# Optional global fallback passkey (applies if owner-specific passkey missing)
# AUTH_MFA_PASSKEY=JBSWY3DPEHPK3PXP
AUTH_SESSION_COOKIE_NAME=cp_session
AUTH_CSRF_COOKIE_NAME=cp_csrf
AUTH_COOKIE_SECURE=true
AUTH_SESSION_TTL_MS=43200000
AUTH_SESSION_IDLE_TIMEOUT_MS=1800000
AUTH_LOGIN_MAX_ATTEMPTS=6
AUTH_LOGIN_WINDOW_MS=600000
AUTH_LOGIN_LOCKOUT_MS=900000
